        "id": "end-activity",
        "description": "When I finish an activity",
        "text": "I finish a fitness activity",
        "generates": [{
            "id": "activity-description",
            "type": "text",
            "description": "the description of the activity",
            "text": "the description of the activity"
        },{"id": "activity-duration",
